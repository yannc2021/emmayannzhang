{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Hugo Server",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "hugo",
        "runtimeArgs": [
          "server",
          "-D",
          "--baseURL=http://localhost:1313/",
          "--appendPort=false",
          "--bind=0.0.0.0"
        ],
        "console": "integratedTerminal"
      }
    ]
  }